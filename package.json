{
  "name": "@containerbase/eslint-plugin",
  "version": "0.0.0-PLACEHOLDER",
  "description": "containerbase eslint plugin",
  "keywords": [
    "eslint-plugin",
    "jest",
    "vitest"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/containerbase/eslint-plugin.git"
  },
  "license": "MIT",
  "author": "Rhys Arkins <rhys@arkins.net>",
  "contributors": [
    "Michael Kriese <michael.kriese@visualon.de>"
  ],
  "type": "module",
  "main": "index.js",
  "files": [
    "index.js",
    "index.d.ts",
    "types"
  ],
  "scripts": {
    "eslint": "eslint --cache .",
    "eslint-fix": "eslint --cache --fix .",
    "lint": "run-s prettier eslint lint:types",
    "lint-fix": "run-s prettier-fix eslint-fix",
    "lint:types": "run-p 'lint:types:*'",
    "lint:types:default": "tsc",
    "prepare": "husky",
    "prettier": "prettier --cache -c -u \"**/*.*\"",
    "prettier-fix": "prettier --cache -w -u \"**/*.*\""
  },
  "devDependencies": {
    "@eslint/js": "9.35.0",
    "@semantic-release/exec": "7.1.0",
    "@tsconfig/node20": "20.1.6",
    "@tsconfig/strictest": "2.0.5",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/node": "22.18.3",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "eslint": "9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-formatter-gha": "1.6.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-promise": "7.2.1",
    "husky": "9.1.7",
    "lint-staged": "16.1.6",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2",
    "prettier-plugin-packagejson": "2.5.19",
    "semantic-release": "24.2.8",
    "typescript": "5.9.2",
    "typescript-eslint": "8.43.0"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-promise": "^7.0.0"
  },
  "packageManager": "pnpm@10.16.1",
  "engines": {
    "node": "^20.9.0 || ^22.11.0",
    "pnpm": "^10.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
