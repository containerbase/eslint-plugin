{
  "name": "@containerbase/eslint-plugin",
  "version": "0.0.0-PLACEHOLDER",
  "description": "containerbase eslint plugin",
  "keywords": [
    "eslint-plugin",
    "jest",
    "vitest"
  ],
  "repository": "https://github.com/containerbase/eslint-plugin.git",
  "license": "MIT",
  "author": "Rhys Arkins <rhys@arkins.net>",
  "contributors": [
    "Michael Kriese <michael.kriese@visualon.de>"
  ],
  "type": "module",
  "main": "index.js",
  "files": [
    "index.js",
    "index.d.ts",
    "types"
  ],
  "scripts": {
    "eslint": "eslint --cache .",
    "eslint-fix": "eslint --cache --fix .",
    "lint": "run-s prettier eslint lint:types",
    "lint:types": "run-p 'lint:types:*'",
    "lint:types:default": "tsc",
    "lint-fix": "run-s prettier-fix eslint-fix",
    "prepare": "husky",
    "prettier": "prettier --cache -c -u \"**/*.*\"",
    "prettier-fix": "prettier --cache -w -u \"**/*.*\""
  },
  "devDependencies": {
    "@eslint/js": "9.26.0",
    "@tsconfig/node20": "20.1.5",
    "@tsconfig/strictest": "2.0.5",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/node": "20.17.42",
    "conventional-changelog-conventionalcommits": "8.0.0",
    "eslint": "9.26.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-formatter-gha": "1.5.2",
    "eslint-import-resolver-typescript": "3.10.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-promise": "7.2.1",
    "husky": "9.1.7",
    "lint-staged": "15.5.2",
    "npm-run-all2": "7.0.2",
    "prettier": "3.5.3",
    "prettier-plugin-packagejson": "2.5.10",
    "semantic-release": "24.2.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.0"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-promise": "^7.0.0"
  },
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": "^20.9.0 || ^22.11.0",
    "pnpm": "^10.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
